<% include ../partial/header.ejs %>
<div class="container">
    <h2 style="text-align:center;">Add new CampGround</h2>
    <div style="width:30%; margin: 20px auto;">
    <form action="/campground" method="POST">
        <div class="form-group">
            <input class="form-control" type="text" name="name" placeholder = "name"/>
        </div>
        <div class="form-group">
            <input class="form-control" type="text" name="location" placeholder = "location"/>
        </div>
        <div class="form-group">
            <input class="form-control" type="text" name="img" placeholder = "img"/>
        </div>
        <div class="form-group">
            <input class="form-control" type="text" name="price" placeholder = "price"/>
        </div>
         <div class="form-group">
            <input class="form-control" type="text" name="desc" placeholder = "desc"/>
        </div>
        <div class="form-group">
        <button class="btn btn-primary btn-lg btn-block"  >Submit!</button>
        </div>
    </form>
    </div>
</div>
<% include ../partial/footer.ejs %>